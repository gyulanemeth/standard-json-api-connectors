{
  "name": "standard-api-connector",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "npx standard",
    "lint-fix": "npx standard --fix",
    "test": "npm run lint && node --experimental-vm-modules node_modules/jest/bin/jest.js --collect-coverage",
    "test-brk": "node --inspect-brk --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "verbose": true,
    "testTimeout": 5000,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gyulanemeth/json-api-connector.git"
  },
  "bugs": {
    "url": "https://github.com/gyulanemeth/json-api-connector/issues"
  },
  "homepage": "https://github.com/gyulanemeth/json-api-connector#readme",
  "dependencies": {
    "qs": "^6.10.5",
    "standard-api-errors": "^0.2.0"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "standard": "^17.0.0"
  },
  "author": "GYN",
  "license": "MIT"
}
